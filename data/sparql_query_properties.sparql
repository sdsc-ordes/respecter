PREFIX skos: <http://www.w3.org/2004/02/skos/core#>
PREFIX owl: <http://www.w3.org/2002/07/owl#>
PREFIX dct: <http://purl.org/dc/terms/>
PREFIX dcat: <https://www.w3.org/TR/vocab-dcat-2/#>
PREFIX sh: <http://www.w3.org/ns/shacl#>
SELECT ?concept ?definition ?label ?example #some reference back to the nodeshape it belongs to
WHERE{
      {
            ?concept a rdf:Property.
        OPTIONAL {?concept skos:definition ?definition}
        OPTIONAL {?concept skos:prefLabel ?label}
        OPTIONAL {?concept skos:example ?example}
        
        }
}
    GROUP BY ?concept ?definition ?label ?example